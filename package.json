{
  "name": "ng-dynamic-forms",
  "version": "6.2.0",
  "description": "A rapid form development library for Angular",
  "scripts": {
    "aot-compile:sample": "ngc -p sample/tsconfig.aot.ngc.json",
    "bundle:sample:rollup": "rollup -c sample/rollup.ngc.config.js",
    "bundle:sample:webpack": "webpack --config sample/webpack.jit.config.js",
    "build:sample": "webpack --config sample/webpack.aot.ngtools.config.js",
    "build:packages": "gulp build",
    "gulp": "gulp",
    "lint:packages": "tslint -c tslint.json './packages/**/*.ts'",
    "publish": "./publish.sh",
    "rollup": "rollup",
    "start": "lite-server -c bs-config.js",
    "server:jt": "webpack-dev-server --config sample/webpack.jit.config.js",
    "tests:e2e": "protractor protractor.conf.js",
    "tests:unit": "karma start karma.conf.js --browsers ChromeHeadless --single-run",
    "tests:unit:debug": "karma start karma.conf.js --browsers Chrome",
    "transpile:sample:es2015": "tsc -p sample/tsconfig.jit.json -m es2015",
    "transpile:sample:commonjs": "tsc -p sample/tsconfig.jit.json -m commonjs",
    "update": "./update.sh",
    "watch:sample": "tsc -p sample/tsconfig.jit.json -m commonjs -watch",
    "webdriver:start": "webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udos86/ng-dynamic-forms.git"
  },
  "author": {
    "name": "Udo Sch√∂fer",
    "email": "ng2-dynamic-forms@udos86.de",
    "url": "https://github.com/udos86/ng-dynamic-forms"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/udos86/ng-dynamic-forms/issues"
  },
  "homepage": "https://github.com/udos86/ng-dynamic-forms#readme",
  
  "devDependencies": {
    "@angular/compiler-cli": "^6.1.9",
    "@ngtools/webpack": "^6.2.4",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.11.3",
    "angular-router-loader": "^0.8.5",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "^4.4.4",
    "dateformat": "^3.0.3",
    "del": "^3.0.0",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-delete-lines": "^0.0.7",
    "gulp-preprocess": "^3.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-run": "^1.7.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typedoc": "^2.2.0",
    "gulp-typescript": "^4.0.2",
    "gulp-util": "^3.0.8",
    "html-minifier": "^3.5.20",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
